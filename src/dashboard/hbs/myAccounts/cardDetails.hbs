<header>
    <button id="backtoaccts">Back</button>
    <span class="account" id="accountName">{{this.accountName}}</span>
    <div class="account-number" id="accountMask">{{this.accountMask}}</div>
    <div id="acct-summary-zone">
        <div class="acct-summary">
            <ul class="leaders">
                <li><span class="acct-summary__label">Payment Due Date</span><span class="acct-summary__value" id="paymentDueDate">{{this.paymentDueDate}}</span></li>
                <li><span class="acct-summary__label">Last Statement Balance</span><span class="acct-summary__value" id="lastStmtBalance">{{this.lastStmtBalance}}</span></li>
                <li><span class="acct-summary__label">Cash APR</span><span class="acct-summary__value" id="cashApr">{{this.cashApr}}</span></li>
                <li><span class="acct-summary__label">Available Credit</span><span class="acct-summary__value" id="availableCredit">{{this.availableCredit}}</span></li>
                <li><span class="acct-summary__label">Purchase APR</span><span class="acct-summary__value" id="purchaseApr">{{this.purchaseApr}}</span></li>
                <li><span class="acct-summary__label">Credit Limit</span><span class="acct-summary__value" id="creditLimit">{{this.creditLimit}}</span></li>
            </ul>
        </div>
        <div style="clear: both;"></div>
    </div>
</header>
